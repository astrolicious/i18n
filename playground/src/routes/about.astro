---
import { t } from "i18n:astro";
import Layout from "~/layouts/Layout.astro";
import Counter from "../components/Counter";
import sitemap from "i18n:astro/sitemap";

sitemap({ 
  optOut: true,
});

const title = t("about");

export const prerender = false;
---

